{
  "expo": {
    "name": "EventIt",
    "slug": "event-manager-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./common/assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSAllowsArbitraryLoads": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.team10students.event-manager-app"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./common/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.roee_b.eventmanagerapp"
    },
    "web": {
      "favicon": "./common/assets/favicon.png"
    }
  }
}
